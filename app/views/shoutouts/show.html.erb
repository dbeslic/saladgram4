<div class="page-header">
  <h3>Shoutout #<%= @shoutout.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">

      <dt>User</dt>
      <dd><%= @shoutout.user_id %></dd>


      <dt>Post</dt>
      <dd><%= @shoutout.post_id %></dd>


      <dt>Comment</dt>
      <dd><%= @shoutout.comment %></dd>


    </dl>

    <div class="btn-group btn-group-justified">

      <a href="/shoutouts" class="btn btn-primary">
        Back
      </a>
      <a href="/shoutouts/<%= @shoutout.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_shoutout/<%= @shoutout.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>

    </div>
  </div>
</div>
